title: $:/plugins/BTC/TiddlyFlex/KeyboardShortcuts/dragging
tags: $:/tags/KeyboardShortcut
key: ((dragging))

<%if [{$:/config/tiddlyflex/story-river/tiddler/drag-enabled}match[no]] %>
	<$action-setfield $tiddler="$:/config/tiddlyflex/story-river/tiddler/drag-enabled" text="yes"/>
<% else %>
	<$action-deletetiddler $tiddler="$:/config/tiddlyflex/story-river/tiddler/drag-enabled"/>
<% endif %>